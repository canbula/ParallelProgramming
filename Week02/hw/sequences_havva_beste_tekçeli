my_list = [2,5,6,7,8]
my_tuple = (3.15 , True , 6)
my_set = {"Star" , "Moon" , "Universe"}
my_dict = {
    "apple" : 240,
    "orange" : 300,
    "strawberry": 400
}

def remove_duplicates(argument : list) -> list:
    return list(set(my_list))

def list_counts(argument:list) -> dict:
    return {item: argument.count(item) for item in set(argument)}

def reverse_dict(d: dict) -> dict:
    return {value:key for key,value in d.items()}
